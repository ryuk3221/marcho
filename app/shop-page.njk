</head>
<body>
  {% include 'app/module/_header/_header.html'%}
  <main class="main">
    <section class="top">
      <div class="top__container" style="background-image: url('images/login-bg.jpg');">
        <div class="container">
          <h2 class="top__title">SHOP PAGE</h2>
          <div class="breadcrumbs">
            <ul class="breadcrumbs__list">
              <li class="breadcrumbs__item">
                <a class="breadcrumbs__link" href="index.html">Home</a>
              </li>
              <li class="breadcrumbs__item">
                <a class="breadcrumbs__link" href="">Shop-page</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>
    <section class="shop">
      <div class="container">
        <div class="shop__inner">
          <div class="filter-search filter__item filter-search--adaptive">
              <h3 class="filter-search__title filter__title">SEARCH</h3>
              <form class="filter-search__form" action="">
                <input class="filter-search__input" type="text" placeholder="Search your keyword....">
                <button class="filter-search__btn" type="submit"><img class="filter-search__img" src="images/search-filter.svg" alt=""></button>
              </form>
            </div>
          <div class="shop__filters filter">
            <button class="filter-btn-adaptive-hide"></button>
            <div class="filter-search filter__item filter-search--hide">
              <h3 class="filter-search__title filter__title">SEARCH</h3>
              <form class="filter-search__form" action="">
                <input class="filter-search__input" type="text" placeholder="Search your keyword....">
                <button class="filter-search__btn" type="submit"><img class="filter-search__img" src="images/search-filter.svg" alt=""></button>
              </form>
            </div>
            <div class="filter__item filter-price">
              <h3 class="filter-price__title">PRICE FILTER</h3>
              <form class="filter-price__form" action="">
                <input type="text" class="js-range-slider filter-price__input" name="my_range" value="" 
                data-type = "double"
                data-min = 0
                data-max = 999
                data-from = 200
                data-to = 800
                data-prefix = '$'
                />
                <label class="filter-price__label">
                  <span>Price: <span class="data-from"></span> - <span class="data-to"></span></span>
                  <button class="filter-price__btn">FILTER</button>
                </label>
              </form>
            </div>
            <div class="filter__item filter-color">
              <h3 class="filter-color__title">COLOR FILTER</h3>
              <form class="filter-color__form" action="#">
                <label class="filter-color__label">
                  <span class="filter-color__text">Blue (15)</span>
                  <div class="filter-color__box">
                    <input class="filter-color__input" type="checkbox">
                    <span class="filter-color__checkbox filter-color__checkbox--blue "></span>
                  </div>
                </label>
                <label class="filter-color__label">
                  <span class="filter-color__text">Red (09)</span>
                  <div class="filter-color__box">
                    <input class="filter-color__input" type="checkbox">
                    <span class="filter-color__checkbox filter-color__checkbox--red"></span>
                  </div>
                </label>
                <label class="filter-color__label">
                  <span class="filter-color__text">Green (28)</span>
                  <div class="filter-color__box">
                    <input class="filter-color__input" type="checkbox">
                    <span class="filter-color__checkbox filter-color__checkbox--green"></span>
                  </div>
                </label>
                <label class="filter-color__label">
                  <span class="filter-color__text">Orange (11)</span>
                  <div class="filter-color__box">
                    <input class="filter-color__input" type="checkbox">
                    <span class="filter-color__checkbox filter-color__checkbox--orange"></span>
                  </div>
                </label>
                <label class="filter-color__label">
                  <span class="filter-color__text">Black (05)</span>
                  <div class="filter-color__box">
                    <input class="filter-color__input" type="checkbox">
                    <span class="filter-color__checkbox filter-color__checkbox--black"></span>
                  </div>
                </label>
                <label class="filter-color__label">
                  <span class="filter-color__text">Purple (21)</span>
                  <div class="filter-color__box">
                    <input class="filter-color__input" type="checkbox">
                    <span class="filter-color__checkbox filter-color__checkbox--purple"></span>
                  </div>
                </label>
              </form>
            </div>
            <div class="filter__item filter-size">
              <h3 class="filter-size__title">SIZE FILTER</h3>
              <form class="filter-size__form" action="">
                <label class="filter-size__label">
                  <div class="filter-size__box">
                    <input class="filter-size__input" type="checkbox" >
                    <span class="filter-size__checkbox"></span>
                  </div>
                  <span class="filter-size__text">X-small</span>
                </label>
                <label class="filter-size__label">
                  <div class="filter-size__box">
                    <input class="filter-size__input" type="checkbox" >
                    <span class="filter-size__checkbox"></span>
                  </div>
                  <span class="filter-size__text">Small</span>
                </label>
                <label class="filter-size__label">
                  <div class="filter-size__box">
                    <input class="filter-size__input" type="checkbox" >
                    <span class="filter-size__checkbox"></span>
                  </div>
                  <span class="filter-size__text">Medium</span>
                </label>
                <label class="filter-size__label">
                  <div class="filter-size__box">
                    <input class="filter-size__input" type="checkbox" >
                    <span class="filter-size__checkbox"></span>
                  </div>
                  <span class="filter-size__text">Large</span>
                </label>
                <label class="filter-size__label">
                  <div class="filter-size__box">
                    <input class="filter-size__input" type="checkbox" >
                    <span class="filter-size__checkbox"></span>
                  </div>
                  <span class="filter-size__text">XXL</span>
                </label>
              </form>
            </div>
            <div class="filter__item filter-category">
              <h3 class="filter-category__title">CATEGORY</h3>
              <form  class="filter-category__form" action="#">
                <label  class="filter-category__label">
                  <input class="filter-category__input"  type="checkbox">
                  <div class="filter-category__checkbox">
                    <span class="filter-category__text">Woman</span>
                    <span class="filter-category__text">48</span>
                  </div>
                </label>
                <label  class="filter-category__label">
                  <input class="filter-category__input"  type="checkbox">
                  <div class="filter-category__checkbox">
                    <span class="filter-category__text">Man</span>
                    <span class="filter-category__text">30</span>
                  </div>
                </label>
                <label  class="filter-category__label">
                  <input class="filter-category__input"  type="checkbox">
                  <div class="filter-category__checkbox">
                    <span class="filter-category__text">Sale products</span>
                    <span class="filter-category__text">92</span>
                  </div>
                </label>
                <label  class="filter-category__label">
                  <input class="filter-category__input"  type="checkbox">
                  <div class="filter-category__checkbox">
                    <span class="filter-category__text">Fashion</span>
                    <span class="filter-category__text">121</span>
                  </div>
                </label>
                <label  class="filter-category__label">
                  <input class="filter-category__input"  type="checkbox">
                  <div class="filter-category__checkbox">
                    <span class="filter-category__text">Hot Dresses</span>
                    <span class="filter-category__text">52</span>
                  </div>
                </label>
                <label  class="filter-category__label">
                  <input class="filter-category__input"  type="checkbox">
                  <div class="filter-category__checkbox">
                    <span class="filter-category__text">Accessories</span>
                    <span class="filter-category__text">88</span>
                  </div>
                </label>
              </form>
            </div>
            <div class="filter__item filter-popular">
              <h3 class="filter-popular__title">POPULAR TAGS</h3>
              <form  class="filter-popular__form" action="#">
                <label class="filter-popular__label">
                  <input class="filter-popular__input" type="checkbox">
                  <div class="filter-popular__checkbox">
                    <span>Sweetshirt</span>
                  </div>
                </label>
                <label class="filter-popular__label">
                  <input class="filter-popular__input" type="checkbox">
                  <div class="filter-popular__checkbox">
                    <span>Man Accessories</span>
                  </div>
                </label>
                <label class="filter-popular__label">
                  <input class="filter-popular__input" type="checkbox">
                  <div class="filter-popular__checkbox">
                    <span>Fashion</span>
                  </div>
                </label>
                <label class="filter-popular__label">
                  <input class="filter-popular__input" type="checkbox">
                  <div class="filter-popular__checkbox">
                    <span>Polo</span>
                  </div>
                </label>
                <label class="filter-popular__label">
                  <input class="filter-popular__input" type="checkbox">
                  <div class="filter-popular__checkbox">
                    <span>T-Shift</span>
                  </div>
                </label>
                <label class="filter-popular__label">
                  <input class="filter-popular__input" type="checkbox">
                  <div class="filter-popular__checkbox">
                    <span>Jewellery</span>
                  </div>
                </label>
              </form>
